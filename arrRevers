// there is an array with numbers. Without using the reverse method, flip its elements in reverse order.

//это мой код

<script>

		var arr = [1,3,2,5,7,44,64,3,56,54657,1234,547];
		var arrrever = [];
		for (var i = 0; i < arr.length; i++){
			arrrever.unshift(arr[i]);
		}
        console.log(arrrever);
   
   </script>
   
   // от разработчиков
   
   var arr = [1, 3, 5, 6, 7, 9];
var result = [];
for (var i = arr.length - 1; i >= 0; i--) {
	result.push(arr[i]);
}
console.log(result);
   
